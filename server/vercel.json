{
  "version": 2,
  "builds": [
    {
      "src": "pgServer.js",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm install"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "pgServer.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

